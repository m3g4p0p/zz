{
  "name": "zeitungzeitung",
  "version": "1.0.0",
  "scripts": {
    "css": "node-sass ./src/scss/main.scss ./static/css/style.css",
    "css:dev": "yarn css && yarn css --watch",
    "js": "webpack",
    "js:dev": "yarn js --watch",
    "js:prod": "yarn js --env.production",
    "dev": "concurrently 'yarn css:dev' 'yarn js:dev'",
    "build": "yarn css && yarn js:prod"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "kanbasu": "^2.5.0",
    "wpapi": "^1.2.1"
  },
  "standard": {
    "env": {
      "browser": true
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties"
    ]
  }
}
