{
  "name": "zeitungzeitung",
  "version": "1.0.0",
  "scripts": {
    "css": "node-sass ./src/scss/main.scss ./static/style.css",
    "css:dev": "yarn css && yarn css --watch",
    "js": "webpack ./src/js/index.js -o ./static/bundle.js",
    "js:dev": "yarn js --mode=development --watch",
    "js:prod": "yarn js --mode=production",
    "dev": "concurrently 'yarn css:dev' 'yarn js:dev'",
    "build": "yarn css && yarn js:prod"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "standard": "^12.0.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "kanbasu": "^2.5.0"
  }
}
